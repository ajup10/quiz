#question3 (1)
dataset<-read.csv(file.choose(),header=T)
#question 3 (b)
dataset1<-na.omit(dataset)
#question3 (c)
plot(gdp~time,xlab="Time",ylab="GDP(%)",main="Singapore GDP growth", type="o", col="blue", data=dataset1)
question 3(f)
lm.fit<-lm(gdp~exp,data=dataset1)                
summary(lm.fit)
